#include <iostream>
 
using namespace std;
 
int main()
 
{
    int t;
    cin>> t;
    while(t--){
            long long int x;
        cin>>x;
        long long int sum=1;
 
            while(x>3)
        {
            sum=sum*2;
            x=x/4;
        }
        cout << sum << endl;
 
    }
 
 
    return 0;
}
